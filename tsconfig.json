{
  "compilerOptions": {
    "plugins": [
      {
        "name": "@effect/language-service"
      }
    ],
    "strict": true,
    "jsx": "react-jsx",
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "target": "ES2022",
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
